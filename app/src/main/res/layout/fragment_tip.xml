<?xml version="1.0" encoding="utf-8"?>
<layout>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.TipFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="60dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="130dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/catbb"
                    android:layout_margin="5dp"
                    android:src="@drawable/bodybuilding"
                    android:layout_width="130dp"
                    android:layout_height="130dp"/>

                <ImageView
                    android:id="@+id/catct"
                    android:layout_margin="5dp"
                    android:src="@drawable/calisthenics"
                    android:layout_width="130dp"
                    android:layout_height="130dp"/>

                <ImageView
                    android:id="@+id/catcf"
                    android:layout_margin="5dp"
                    android:src="@drawable/crossfit"
                    android:layout_width="130dp"
                    android:layout_height="130dp"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="130dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/catpl"
                    android:layout_margin="5dp"
                    android:src="@drawable/powerlift"
                    android:layout_width="130dp"
                    android:layout_height="130dp"/>

                <ImageView
                    android:id="@+id/catrn"
                    android:layout_margin="5dp"
                    android:src="@drawable/running"
                    android:layout_width="130dp"
                    android:layout_height="130dp"/>

                <ImageView
                    android:id="@+id/allProg"
                    android:layout_margin="5dp"
                    android:src="@drawable/all"
                    android:layout_width="130dp"
                    android:layout_height="130dp"/>

            </LinearLayout>




        </LinearLayout>

        <LinearLayout
            android:background="#858585"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            app:layout_constraintBottom_toBottomOf="parent">

            <ImageView
                android:id="@+id/homeTap"
                android:layout_weight="1"
                android:src="@drawable/logo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <ImageView
                android:background="@color/black"
                android:id="@+id/tipTap"
                android:layout_weight="1"
                android:src="@drawable/logo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <ImageView
                android:id="@+id/talkTap"
                android:layout_weight="1"
                android:src="@drawable/logo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>


            <ImageView
                android:id="@+id/bookmarkTap"
                android:layout_weight="1"
                android:src="@drawable/logo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <ImageView
                android:id="@+id/storeTap"
                android:layout_weight="1"
                android:src="@drawable/logo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>


        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>

</layout>